{
  "expo": {
    "name": "LinusPlaylists",
    "slug": "music-player",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": false,
    "scheme": [
      "com.sunil.music-player",
      "com.msunilhyd.musicplayer",
      "com.googleusercontent.apps.472641857686-qcnd1804adma81q7j7o7t6ge9e80alkt"
    ],
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "usesAppleSignIn": true,
      "infoPlist": {
        "UIBackgroundModes": [
          "audio"
        ],
        "AVAudioSessionCategory": "AVAudioSessionCategoryPlayback",
        "NSAppleMusicUsageDescription": "This app plays music",
        "ITSAppUsesNonExemptEncryption": false
      },
      "bundleIdentifier": "com.sunil.music-player"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.FOREGROUND_SERVICE",
        "android.permission.WAKE_LOCK"
      ],
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.msunilhyd.musicplayer"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "454fd06b-a3de-441b-b594-028675095b45"
      }
    }
  }
}
